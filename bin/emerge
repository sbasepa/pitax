#!/usr/bin/python3
import sys
import os
import subprocess

try:
    if (os.path.exists(f'/root/k/Pitax/etc/pitax/code/{(sys.argv[1])[2:]}.py')):
        subprocess.run(['python3', f'/root/k/Pitax/etc/pitax/code/{(sys.argv[1])[2:]}.py', ' '.join(sys.argv[2:])], check=True)    
    else:
        print("No action found for", sys.argv[1])
except Exception as e:
    print("Error: ", e)
